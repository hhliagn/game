<?xml version="1.0" encoding='UTF-8'?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.game.role.player.entity">
    <class name="com.game.role.player.entity.PlayerEnt" table="player">
        <id name="id" column="id" type="java.lang.Long">
            <generator class="assigned"/>
        </id>
        <property name="accountId" column="accountId" type="java.lang.String"/>
        <property name="alive" column="alive" type="java.lang.Boolean"/>
    </class>
</hibernate-mapping>