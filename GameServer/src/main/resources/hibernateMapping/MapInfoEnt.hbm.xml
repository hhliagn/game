<?xml version="1.0" encoding='UTF-8'?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.game.user.mapInfo.entity">
    <class name="com.game.user.mapInfo.entity.MapInfoEnt" table="mapinfo">
        <id name="id" column="id" type="int">
            <generator class="identity"/>
        </id>
        <property name="accountId" column="accountId" type="java.lang.String"/>
        <property name="curMapId" column="curMapId" type="int"/>
        <property name="lastMapId" column="lastMapId" type="int"/>
    </class>
</hibernate-mapping>