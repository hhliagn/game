<?xml version="1.0" encoding='UTF-8'?>

        <!DOCTYPE hibernate-mapping PUBLIC
                "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.game.scene.entity">
<class name="com.game.scene.entity.NpcEnt" table="npc">
    <id name="id" column="id" type="java.lang.Long">
        <generator class="assigned"/>
    </id>
    <property name="name" column="name" type="java.lang.String"/>
    <property name="mapId" column="mapId" type="int"/>
    <property name="x" column="x" type="int"/>
    <property name="y" column="y" type="int"/>
    <property name="status" column="status" type="int"/>
</class>
</hibernate-mapping>